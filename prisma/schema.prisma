// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model link{
  id    Int     @id @default(autoincrement())
  createAt  DateTime @default(now())
  description   String
  url   String
}
